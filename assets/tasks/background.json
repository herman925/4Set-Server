{
  "id": "background",
  "title": "背景資料",
  "entryForm": [
      {
        "id": "student-id",
        "type": "text",
        "label": "學生編號",
        "placeholder": "輸入學生編號",
        "validation": {
          "regex": "^[0-9]{5}$",
          "errorMessage": "學生編號必須是5位數字。"
        }
      },
    {
      "id": "child-name",
      "type": "text",
      "label": "姓名",
      "placeholder": "輸入學生編號自動填寫，可自行修改",
      "required": true
    },
    {
      "id": "gender",
      "type": "select",
      "label": "性別",
      "options": [
        {
          "value": "M",
          "label": "男"
        },
        {
          "value": "F",
          "label": "女"
        }
      ]
    },
    {
      "id": "school-id",
      "type": "text",
      "label": "學校編號",
      "placeholder": "輸入學校編號",
      "validation": {
        "min": 1,
        "max": 115,
        "errorMessage": "學校編號必須是1到115之間的數字。"
      }
    },
    {
      "id": "school-name",
      "type": "text",
      "label": "學校名稱",
      "placeholder": "輸入學校編號後自動填寫"
    },
    {
      "id": "class-id",
      "type": "text",
      "label": "班別編號",
      "placeholder": "輸入班別編號"
    },
    {
      "id": "class-name",
      "type": "text",
      "label": "班別名稱",
      "placeholder": "輸入班別編號後自動填寫"
    }
  ],
  "questions": []
}